<view class="content"><view class="tui-searchbox"><view class="tui-search-input"><icon type="search" size="{{13}}" color="#333"></icon><input class="tui-input" confirm-type="search" placeholder="搜索昵称/备注" focus="{{true}}" auto-focus="{{true}}" placeholder-class="tui-input-plholder" data-event-opts="{{[['keydown',[['searchFn',['$event']]]],['input',[['__set_model',['$0','search','$event',['trim']],['query']]]],['blur',[['$forceUpdate']]]]}}" value="{{query.search}}" bindkeydown="__e" bindinput="__e" bindblur="__e"/></view><view data-event-opts="{{[['tap',[['searchFn',['$event']]]]]}}" class="tui-cancle" bindtap="__e">搜索</view></view><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><tui-list-cell vue-id="{{'76cc9ebe-1-'+i}}" lineLeft="{{false}}" bind:__l="__l" vue-slots="{{['default']}}"><view class="tui-item-box"><view class="tui-msg-box"><image class="tui-msg-pic" src="{{item.avatarUrl}}" mode="widthFix"></image><view class="tui-msg-item"><view class="tui-msg-name">{{item.nickName}}</view><block wx:if="{{isAdmin}}"><view class="tui-msg-content">备注：<block wx:if="{{item.showEdit}}"><input class="tui-input-note" type="text" focus="{{focus}}" data-event-opts="{{[['blur',[['updateNote',['$0',i],[[['dataList','',i]]]]]],['input',[['__set_model',['$0','note','$event',[]],[[['dataList','',i]]]]]]]}}" value="{{item.note}}" bindblur="__e" bindinput="__e"/></block><block wx:else><label class="tui-text-note _span">{{item.note?item.note:'他/她还没有备注'}}</label></block><block wx:if="{{!item.showEdit}}"><tui-icon vue-id="{{('76cc9ebe-2-'+i)+','+('76cc9ebe-1-'+i)}}" name="evaluate" size="{{20}}" color="#333" margin="15rpx" data-event-opts="{{[['^click',[['edit',['$0',i],[[['dataList','',i]]]]]]]}}" bind:click="__e" bind:__l="__l"></tui-icon></block></view></block><block wx:else><view class="tui-msg-content"><label class="_span">{{"积分："+item.integral}}</label></view></block></view></view><block wx:if="{{isAdmin}}"><view class="tui-msg-right"><tui-numberbox vue-id="{{('76cc9ebe-3-'+i)+','+('76cc9ebe-1-'+i)}}" value="{{item.integral}}" index="{{i}}" min="{{0}}" max="{{1000000}}" height="{{60}}" width="{{70}}" iconSize="{{30}}" data-event-opts="{{[['^change',[['change',['$event','$0'],[[['dataList','',i]]]]]]]}}" bind:change="__e" bind:__l="__l"></tui-numberbox></view></block><block wx:else><view class="tui-msg-right"><label class="rank _span">{{i+1}}</label></view></block></view></tui-list-cell></block></view>